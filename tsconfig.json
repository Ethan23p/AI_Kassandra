{
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "Preserve",
    "target": "esnext",
    "moduleResolution": "bundler",
    "moduleDetection": "force",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,

    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",

    "types": ["bun-types"]
  }
}